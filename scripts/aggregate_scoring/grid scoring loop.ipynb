    "# --- Main Scoring Function ---\n",
    "def process_point(lat, lon, kwargs):\n",
    "    try:\n",
    "        ct_score = CommunityTransportationOptions(lat, lon, **kwargs).calculate_score()\n",
    "        dua_score = DesirableUndesirableActivities(lat, lon, **kwargs).calculate_score()\n",
